@model LikeFilms.ViewModel.StatisticsViewModel;

<ul class="list-group">
    <li class="list-group-item active"><a class="text-black" asp-action="Index" asp-controller="TvShows" asp-route-byGenre="">Total TV shows: @Model.TotalCount</a></li>
    @foreach (KeyValuePair<Genre, int> item in Model.GenreCount)
    {
        <li class="list-group-item"><a asp-action="Index" asp-controller="TvShows" asp-route-byGenre="@item.Key">@item.Key - @item.Value</a></li>
    }
</ul>
